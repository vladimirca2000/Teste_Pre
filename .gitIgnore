# .gitignore para projetos Angular e .NET

# Node
node_modules/

# Angular build outputs
dist/
build/

# Angular environment files
src/environments/*.ts

# IDEs e editores
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln
*.swp

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Diretórios temporários
.tmp/

# Arquivos de configuração do sistema
.DS_Store
Thumbs.db

# .NET
bin/
obj/

# Pacotes NuGet
*.nupkg
*.snupkg

# Arquivos de usuário do Visual Studio
*.user
*.userosscache
*.suo
*.sln.docstates

# Diretórios de publicação
publish/

# Arquivos de configuração secretos
appsettings.*.json

# Testes e relatórios
TestResults/
*.coverage
*.coveragexml

# Outros arquivos comuns
*.log
*.vspscc
*.vssscc

# Ferramentas de build
/.angular/cache
/.angular/*

# Arquivos de backup
*.bak
*.tmp
